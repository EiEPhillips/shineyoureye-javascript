{
  "name": "shineyoureye-javascript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prose": "node bin/prepare-data.js",
    "serve": "vue-cli-service serve",
    "build": "npm run prose && vue-cli-service build",
    "clean": "rm -rf node_modules",
